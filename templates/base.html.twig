<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title>
            {% block title %}
                Welcome!
            {% endblock %}
        </title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        {% if app.environment == 'prod' %}
            <script async
                defer
                data-domain="pezos.fi"
                src="https://pezos-stats.duckdns.org/js/plausible.js"></script>
        {% endif %}
    </head>
    <body class="dark">
        <div class="container">
            {% include '_navbar.html.twig' %}
            {% block body %}

            {% endblock %}
        </div>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
